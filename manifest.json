{
    "manifest_version": 2,

    "name": "PlugDJ Youtube Importer",
    "description": "유튭주소로 PlugDJ에 강제추가.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },

    "permissions": [
    "clipboard",
    "contextMenus",
    "<all_urls>"
  ]
}
